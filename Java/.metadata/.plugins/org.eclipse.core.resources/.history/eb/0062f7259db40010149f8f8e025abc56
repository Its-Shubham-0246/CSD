import java.util.List;

public class FunctionalProgrammingRunner {
	public static void main(String[] args) {
		List<Integer> list = List.of(1, 2, 3, 4, 5, 6, 7, 8);
		int sum = list.stream().reduce(0, (num1, num2) -> num1 + num2);
		System.out.println(sum);
//		
//		IntStream.range(1, 10).map(element -> element * element).forEach(element -> System.out.println(element));
	}

//	private static void printList(List<String> list) {
//		list.stream().forEach(element -> System.out.println(element));
//	}
//
//	private static void printListFiltering(List<String> list) {
//		list.stream().filter(element -> element.endsWith("at")).forEach(element -> System.out.println(element));
//	}
}
